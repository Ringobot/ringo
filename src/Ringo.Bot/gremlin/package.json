  "devDependencies": {
    "greenkeeper-postpublish": "^1.0.1",
    "gremlin": "^2.3.2",
    "scmp": "^2.0.0",
    "ts-node": "^4.0.0",
    "tslint": "^5.0.0",
    "tslint-config-typings": "^0.3.0",
    "typescript": "^2.0.3",
